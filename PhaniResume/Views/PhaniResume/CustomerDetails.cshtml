@model webapi.proxies.Models.CustomerDetail
<div id="CustomerDetails" class="panel panel-success mvc-form">
    <div class="panel-heading">
        <div >
            <h3 class="panel-title">
                Applicant Personal Details
            </h3>
        </div>
    </div>
    <div class="panel-body mvc-element panel-collapse in" id="CustomerDetailsbody">
        <div class="form-group">
            <div class="form-group row">
                <label class="col-sm-2">First Name </label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(x=>x.FirstName , new {@class= "form-control"})
                </div>
            </div>
            
            <div class="form-group row">
                <label class="col-sm-2"> Last Name</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(x => x.LastName, new { @class = "form-control" })
                </div>
            </div>
            
            <div class="form-group row">
                <label class="col-sm-2"> phone Number</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(x => x.phoneNumber, new { @class = "form-control" , type = "number"})
                </div>
            </div>
            
            <div class="form-group row">
                <label class="col-sm-2">Zipcode</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(x => x.Zipcode, new { @class = "form-control", type = "number" })
                </div>
            </div>
        </div>
        
        <div id="Resume-Details-Summary"></div>

    </div>
</div>


@section scripts {
    <script>
        $(function() {
            ResumeDetials.init({
                customerId: 1 ,
                applicationPath :"@Request.ApplicationPath"
            });
        });
    </script>

}


